# LinkedIn OAuth Credentials
# Get these from https://developer.linkedin.com/
LINKEDIN_CLIENT_ID=your_client_id_here
LINKEDIN_CLIENT_SECRET=your_client_secret_here

# OAuth redirect URI (must match what's configured in LinkedIn Developer Portal)
LINKEDIN_REDIRECT_URI=http://localhost:3001/api/auth/linkedin/callback

# Frontend URL (for CORS and OAuth redirect)
FRONTEND_URL=http://localhost:5173

# Server port
PORT=3001

# Session secret (auto-generated if not set)
SESSION_SECRET=your_random_secret_here
